issn = {15821838},
journal = {Journal of Cellular and Molecular Medicine},
keywords = {AOCS,Cited,NMF,Network,Ovarian,bioinformatics,clusters,lipid metabolism,microarray,obesity,ovarian cancer,survival statistics},
mendeley-tags = {AOCS,Cited,NMF,Network,Ovarian},
number = {3},
