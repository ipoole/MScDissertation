publisher = {NIH Public Access},
title = {{Molecular classification of high grade endometrioid and clear cell ovarian cancer using TCGA gene expression signatures.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27016234 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5616158},
